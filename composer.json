{
    "name": "kulasis/standard-edition",
    "homepage": "http://kulasis.org",
    "license": "MIT",
    "type": "project",
    "description": "The \"Kula SIS Standard Edition\" distribution",
    "authors": [
        {
            "name": "Makoa Jacobsen",
            "email": "makoa@makoajacobsen.com",
            "homepage": "http://www.makoajacobsen.com/"
        }
    ],
    "autoload": {
        "psr-0": { 
          "": "src/" 
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "kulasis/kulasis": "dev-master"
    },
    "extra": {
      "symfony-app-dir": "app",
      "symfony-bin-dir": "bin",
      "symfony-var-dir": "var",
      "symfony-web-dir": "web",
      "symfony-tests-dir": "tests",
      "symfony-assets-install": "relative"
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/kulasis/kulasis"
      }
    ]
}
